<script is:inline>
    const MAX_COLS = 4;
    const stepsData = [
        { columns: 4, headline: "Where is your business today?", items: [
            { title: "Startup", subtitle: "Just getting started" },
            { title: "Growth", subtitle: "Scaling in your market" },
            { title: "Expansion", subtitle: "Entering new markets" },
            { title: "Established", subtitle: "Strengthening your position" },
        ]},
        { columns: 3, headline: "What's your priority right now?", items: [
            { title: "Branding & Identity" },
            { title: "Website / Digital presence" },
            { title: "Full digital solution" },
        ]},
        { columns: 4, headline: "When are you looking to take action?", items: [
            { title: "Immediately" },
            { title: "Within 3 months" },
            { title: "Within 6 months" },
            { title: "Just exploring" }
        ]},
        { columns: 4, headline: "What's the biggest obstacle you're facing?", items: [
            { title: "Blending in with competition" },
            { title: "Website not converting" },
            { title: "Systems blocking growth" },
            { title: "Can't rely on current agency" }
        ]},
        { columns: 3, headline: "What level of investment are you ready to make?", items: [
            { title: "€10k - €25k" },
            { title: "€25k - €50k" },
            { title: "€50k+" }
        ]}
    ];

    const row = document.getElementById('gts-row');
    const headline = document.getElementById('gts-headline');
    const lastStep = document.getElementById('gts-last-step');
    const progressStepTxt = document.querySelector(".fm-progress-txt p span:nth-child(2)");
    const progressBar = document.querySelector(".fm-progress-bar-spin");
    const prevBtn = document.querySelector(".gs-prev-step");
    const pgBarMain = document.querySelector(".gs-form-progress-bar");

    let step = 0;
    let answers = [];

    for (let i = 0; i < MAX_COLS; i++) {
        const col = document.createElement('div');
        col.className = 'gts-form-col';
        if(row){
            row.appendChild(col);
        }
    }

    function animateWidths(columns) {
        const cols = [...row.children];

        cols.forEach(col => {
            col.style.width = col.getBoundingClientRect().width + 'px';
            col.style.transition = 'width .5s cubic-bezier(.4,0,.2,1)';
        });

        row.offsetHeight;

        const width = 100 / columns + '%';
        cols.forEach(col => {
            col.style.width = width;
        });

        setTimeout(() => {
            cols.forEach(col => {
                col.style.transition = '';
                col.style.width = '';
            });
        }, 500);
    }

    function playBoxVideo(col) {
        document.querySelectorAll(".sc-anim").forEach(a => {
            a.classList.remove("active");
            a.style.pointerEvents = "none";
        });

        document.querySelectorAll(".bg-video").forEach(v => {
            v.pause();
            v.currentTime = 0;
        });

        const anim = col.querySelector(".sc-anim");
        const video = col.querySelector(".bg-video");

        if (!anim || !video) return;

        anim.classList.add("active");
        anim.style.pointerEvents = "auto";
        video.play();
    }

    function renderStep() {
        const data = stepsData[step];

        headline.style.opacity = 0;
        setTimeout(() => {
            headline.textContent = data.headline;
            headline.style.opacity = 1;
        }, 150);

        progressStepTxt.textContent = step + 1;
        progressBar.style.width = `${((step + 1) / stepsData.length) * 100}%`;

        prevBtn.classList.toggle("hidden", step === 0);

        row.classList.remove('threecol','fourcol');
        row.classList.add(data.columns === 3 ? 'threecol' : 'fourcol');

        animateWidths(data.columns);

        [...row.children].forEach((col, i) => {
            if (i < data.items.length) {
                const item = data.items[i];
                col.style.display = 'block';
                col.innerHTML = `
                    <div class="gts-form-col-wrap fx fx-wrap fx-ae">
                        <div class="gts-form-headline fwidth">
                            <h3>${item.title}</h3>
                            ${item.subtitle ? `<h5>${item.subtitle}</h5>` : ''}
                        </div>
                        <div class="gts-form-num fwidth">
                            <span>${String(i+1).padStart(2,'0')}</span>
                        </div>
                    </div>
                    <div class="sc-anim" style="opacity:0;pointer-events:none;">
                        <video muted playsinline loop class="bg-video">
                            <source src="/vd/success-anim.mp4" type="video/mp4">
                        </video>
                    </div>
                `;

                let stepTimeout;

                col.onclick = () => {
                    answers[step] = item.title;
                    step++;
                    if (step < stepsData.length) {
                        col.classList.add("click-animate");
                        col.addEventListener('animationend', () => {
                            col.classList.remove("click-animate");
                        }, { once: true });

                        playBoxVideo(col);
                        stepTimeout = setTimeout(() => {
                            clearTimeout(stepTimeout);
                            renderStep();
                        }, 1500);
                    } else {

                        col.classList.add("click-animate");
                        col.addEventListener('animationend', () => {
                            col.classList.remove("click-animate");
                        }, { once: true });

                        playBoxVideo(col);
                        stepTimeout = setTimeout(() => {
                            clearTimeout(stepTimeout);
                            row.style.display = 'none';
                            headline.style.display = 'none';
                            pgBarMain.style.display = 'none';
                            lastStep.style.display = 'block';
                            requestAnimationFrame(() => lastStep.style.opacity = 1);
                        }, 1400);
                    }
                };
            } else {
                col.style.display = 'none';
            }
        });
    }

    if(prevBtn){
        prevBtn.onclick = () => {
            if (step === 0) return;
            step--;
            renderStep();
        };
    }

    if(row){
        renderStep();
    }

</script>
