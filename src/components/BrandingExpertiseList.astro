<script>
    document.addEventListener('DOMContentLoaded', () => {
        document.querySelectorAll('.exp-acc-item').forEach(item => {
            const header = item.querySelector('.exp-acc-top');
            const content = item.querySelector('.exp-acc-main');

            item.classList.add('opened');
            content.style.maxHeight = content.scrollHeight + 'px';

            header.addEventListener('click', () => {
            const isOpen = item.classList.contains('opened');

            if (isOpen) {
                item.classList.remove('opened');
                content.style.maxHeight = '0px';
            } else {
                item.classList.add('opened');
                content.style.maxHeight = content.scrollHeight + 'px';
            }
            });
        });
        window.addEventListener('resize', () => {
            document.querySelectorAll('.exp-acc-item.opened .exp-acc-main').forEach(el => {
                el.style.maxHeight = el.scrollHeight + 'px';
            });
        });
    });
</script>